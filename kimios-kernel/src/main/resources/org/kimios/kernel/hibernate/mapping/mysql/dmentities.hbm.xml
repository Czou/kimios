<entity-mappings
        xmlns="http://java.sun.com/xml/ns/persistence/orm"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm orm_2_0.xsd"
        version="2.0">
    <entity class="org.kimios.kernel.dms.DMEntityImpl" metadata-complete="false">
        <attribute-override name="path">
            <column name="dm_entity_path" length="255" unique="true"/>
        </attribute-override>
        <attribute-override name="addOnDatas">
            <column nullable="true" name="dm_entity_addon_data" length="5000"  />
        </attribute-override>
    </entity>
</entity-mappings>
